{
  "name": "GPT-3 Writer",
  "description": "Write your blog title here and we'll optimize it & write a short blog post anywhere!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "SEO Keyword Generator"
  },
  "permissions": ["contextMenus", "tabs", "storage"]
}
